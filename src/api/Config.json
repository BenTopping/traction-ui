{
  "sequencescape": {
    "name": "sequencescape",
    "apiNamespace": "api/v2",
    "resources": {
      "requests": {
        "name": "requests",
        "filter": {
          "type": "long_read",
          "state": "pending"
        },
        "include": "samples.sample_metadata"
      },
      "tubes": {
        "name": "tubes"
      }
    }
  },
  "traction": {
    "name": "traction",
    "apiNamespace": "v1",
    "resources": {
      "samples": {
        "name": "samples"
      },
      "libraries": {
        "name": "libraries"
      },
      "enzymes": {
        "name": "enzymes"
      },
      "runs": {
        "name": "runs",
        "include": "chip.flowcells.library"
      },
      "chips": {
        "name": "chips"
      },
      "flowcells": {
        "name": "flowcells"
      },
      "tubes": {
        "name": "tubes",
        "include": "material"
      }
    }
  }
}
